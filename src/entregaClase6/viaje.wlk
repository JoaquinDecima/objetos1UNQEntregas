class Viaje{
	var destino 
	var fecha
	
	constructor(sDestino,nFecha){
		destino = sDestino
		fecha = nFecha
	}
	
	// Retorna el destino
	method destino(){
		return destino
	}
	
	// Retorna la fecha
	method fecha(){
		return fecha
	}
}